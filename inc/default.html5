<h1>$subtitle$</h1>
$endif$
$for(author)$
<h2>$author$</h2>
$endfor$
$if(date)$
<div id="date">$date$</div>
$endif$
</div>
</header>
$endif$
<div id="cartoon">
    <img src="img/deviant-code-1000px.png" alt="cartoon">
</div>
$if(toc)$
<nav id="$idprefix$TOC">
$toc$
</nav>
$endif$
$body$
$for(include-after)$
$include-after$
$endfor$
</body>
</html>
